// тестовая схема

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int      @id @default(autoincrement())
  username     String   @unique
  passwordHash String
}

model RoomCodes {
  id           Int      @id @default(autoincrement())
  longRoomId   String   
  shortRoomId  String   @unique
}