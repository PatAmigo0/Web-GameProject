generator client {
  provider = "prisma-client-js"
  engineType = "library"
}

datasource db {
  provider = "postgresql"
}

model User {
  id           Int      @id @default(autoincrement())
  username     String   @unique
  uuid         String   @unique

  passwordHash String? 

  googleId     String?  @unique
  githubId     String?  @unique
}

model RoomCodes {
  id           Int      @id @default(autoincrement())
  longRoomId   String   @unique
  shortRoomId  String   @unique
}